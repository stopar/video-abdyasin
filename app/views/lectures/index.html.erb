<% if current_moderator %>

	<div id="moderator_actions">
		<div class="row-fluid">
			<div class="span9">
				<h1>Laman moderator</h1>
			</div>
		
			<div class="span3 upload_btn">
					<%= link_to "Upload ceramah baru", new_lecture_path, 
						:class => "btn btn-large btn-primary" %>
			</div>
		
			<% if flash.any? %>
				<div class="span10 alert <%= "alert-success" if flash[:success] %>
										 <%= "alert-error" if flash[:error] %>">
					<a class="close" data-dismiss="alert">Ã—</a>
					<% if flash[:success] %>
					    <%= flash[:success] %>
				    <% end %>
	
				    <% if flash[:error] %>
						<%= flash[:error] %>
				    <% end %>
				</div>
			<% end %>
		
		</div>
	</div>
	
<% end %>	

<div id="lectures_thumbnails">
	<ul class = "thumbnails">
		<%= render @lectures %>
	</ul>
</div>









